{
  "expo": {
    "name": "Farm2Table",
    "slug": "Farm2Table",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rafiki6334.Farm2Table",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.rafiki6334.Farm2Table",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
